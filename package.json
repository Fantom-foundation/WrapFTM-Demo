{
    "name": "wrapftm",
    "version": "0.0.0",
    "scripts": {
        "dev": "vite",
        "dev:mocked": "cross-env VITE_ENABLE_MOCKING=1 vite",
        "dev:undermaintenance": "cross-env VITE_UNDER_MAINTENANCE=1 vite",
        "build": "vite build",
        "build:mocked": "cross-env VITE_ENABLE_MOCKING=1 vite build",
        "build:mocks": "vite build --config tests/utils/mocks/vite.config.mjs",
        "build:undermaintenance": "cross-env VITE_UNDER_MAINTENANCE=1 vite build",
        "preview": "vite preview --port 5050",
        "ttt": "vite --help",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
        "test": "cross-env TZ=UTC vitest run --environment jsdom",
        "test:watch": "cross-env TZ=UTC vitest --environment jsdom",
        "test:updatesnapshots": "cross-env TZ=UTC vitest -- -u",
        "test:coverage": "cross-env TZ=UTC vitest run --coverage",
        "test:report": "cross-env TZ=UTC vitest run --environment jsdom --reporter json --outputFile test_results.json",
        "test:all": "npm-run-all test test:acceptance:ci",
        "template": "plop",
        "prepare": "husky install",
        "install:web3-wallets": "cd src/plugins/web3-wallets && npm install",
        "install:mocks": "npm run build:mocks",
        "postinstall": "npm-run-all install:*"
    },
    "dependencies": {
        "@apollo/client": "^3.8.8",
        "@vueuse/core": "^10.7.1",
        "bignumber.js": "^9.1.2",
        "cross-fetch": "^3.1.5",
        "dayjs": "^1.11.10",
        "dotenv": "^16.3.1",
        "ethers": "^5.7.2",
        "fantom-vue3-components": "^0.37.13",
        "graphql": "^16.8.1",
        "graphql-tag": "^2.12.6",
        "node-stdlib-browser": "^1.2.0",
        "pinia": "^2.1.7",
        "vue": "^3.4.5",
        "vue-i18n": "^9.8.0",
        "vue-router": "^4.2.5"
    },
    "devDependencies": {
        "@commitlint/cli": "^18.4.3",
        "@commitlint/config-conventional": "^18.4.3",
        "@depay/web3-mock": "^14.17.0",
        "@pinia/testing": "^0.1.3",
        "@rollup/plugin-inject": "^5.0.3",
        "@rushstack/eslint-patch": "^1.2.0",
        "@vitejs/plugin-basic-ssl": "^1.1.0",
        "@vitejs/plugin-vue": "^5.0.4",
        "@vue/eslint-config-prettier": "^9.0.0",
        "@vue/test-utils": "^2.4.3",
        "cross-env": "^7.0.3",
        "eslint": "^8.56.0",
        "eslint-plugin-vue": "^9.19.2",
        "husky": "^8.0.3",
        "jsdom": "^23.0.1",
        "lint-staged": "^15.2.0",
        "npm-run-all": "^4.1.5",
        "plop": "^4.0.1",
        "prettier": "^3.1.1",
        "sass": "^1.69.7",
        "vite": "^5.1.4",
        "vite-plugin-pwa": "^0.19.0",
        "vitest": "^1.3.1"
    },
    "lint-staged": {
        "*.{vue,js,jsx,cjs,mjs}": "eslint --cache --fix",
        "*.{js,css,md}": "prettier --write"
    }
}
